doctype html
html(lang='en')
  head
    title World Wide Repository
    style(type='text/css').
      * {
        background-color: rgba(255,0,255,1);
      }
      div {
        width: 100%;
        height: 100%;
        margin: auto auto;
        padding: auto auto;
        text-align: left;
        font-family: helvetica;
        font-size: 300px;
        color: rgba(0, 0, 255, 1);
        background-color: rgba(255, 255, 0);
      }
  body
    div
      each entry in dbColEntries
        h1= entry.name
        p= entry._id